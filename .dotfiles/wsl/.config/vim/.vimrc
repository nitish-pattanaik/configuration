filetype indent on
set nobackup
set noswapfile
set ignorecase
set smartcase
set smartindent
set confirm

set wrap
set expandtab
set tabstop=8
set shiftwidth=4
set softtabstop=4

set encoding=utf-8
set fileencoding=utf-8
set fileformat=unix
set number
set rnu
set title

set termguicolors
colorscheme pablo
hi Statement guifg=#f1c21b
hi! Identifier guifg=#9ef0f0
hi! Function guifg=#3ddbd9
hi Special guifg=darkmagenta
hi PreProc guifg=#a56eff
"Costant = String + Character + Number + Boolean + Float
hi String guifg=#a2a9b0
hi Number guifg=#ff832b
hi Comment guifg=#78a9ff
"hi Type guifg=#ff832b
hi! Normal guibg=#161616 guifg=#e4e4ef
hi Todo guifg=#000000 guibg=#f1c21b
hi Error guifg=#da1e28
hi Underlined guifg=#82cfff
set cursorlineopt=number
hi LineNr guifg=#4d5358 guibg=#161616

